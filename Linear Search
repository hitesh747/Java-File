class Main {
    static int my_fun(int[] arr,int tar){
        for(int i=0; i<arr.length; i++){
            if(arr[i] == tar){
                return i;
            }
        }
        return -1;
    }
    public static void main(String[] args) {
        int[] arr = {2,5,6,7,8,9,1,3};
        int tar = 8;
        int result = my_fun(arr,tar);
        System.out.println(result);
    }
}
